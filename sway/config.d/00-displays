# vim: set filetype=swayconfig :
# Output configs

# Laptop display
output eDP-1 {
    mode 1920x1080
    position 0 1080
}

# VGA
output VGA-1 {
    mode 1920x1080
    position 1920 1080
}

# DisplayLink dock (work)
output HDMI-1-0 {
    mode 1920x1080
    position 0 0
}

output HDMI-1-1 {
    mode 1920x1080
    position 1920 0
}
